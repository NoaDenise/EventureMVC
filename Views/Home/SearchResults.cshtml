@model List<string>

@{
    ViewData["Title"] = "Search Results";
}

<h1>Search Results</h1>

@if (Model.Any())
{
    <ul class="search-list">
        @foreach (var view in Model)
        {
            <li>
                <a href="/@view">@view</a>
            </li>
        }
    </ul>
}
else
{
    <p>No views found matching your search.</p>
}
