
<div class="list-group">
    <!-- My Information -->
    <a asp-controller="Admin" asp-action="ListAdminInformation" class="list-group-item list-group-item-action list-group-item-info">
        My Information
    </a>

    <!-- Activities to Approve -->
    <a asp-controller="Admin" asp-action="ListActivitiesToApprove" class="list-group-item list-group-item-action list-group-item-warning">
        Activities to Approve
    </a>

    <!-- List/Edit/Delete Activities -->
    <a asp-controller="Admin" asp-action="ListAllActivities" class="list-group-item list-group-item-action list-group-item-primary">
        List/Edit/Delete Activities
    </a>

    <!-- List/Create/Edit/Delete Categories -->
    <a asp-controller="Admin" asp-action="ListAllCategories" class="list-group-item list-group-item-action list-group-item-secondary">
        List/Create/Edit/Delete Categories
    </a>
</div>


@if (TempData["ErrorMessage"] != null)
{
    <div class="alert alert-danger">
        @TempData["ErrorMessage"]
    </div>
}